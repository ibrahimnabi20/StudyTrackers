{
  "stryker-config": {
    "project": "backend/StudyTracker.csproj",
    "test-projects": [
      "tests/UnitTests/UnitTests.csproj"
    ],
    "solution": "../StudyTrackers.sln",
    "reporters": [
      "html",
      "progress"
    ],
    "coverage-analysis": "perTest",
    "thresholds": {
      "high": 90,
      "low": 80,
      "break": 80
    },
    "concurrency": 1,
    "language-version": "latest",
    "mutate": [
      "**/*.cs",
      "!**/Startup.cs",
      "!**/Program.cs",
      "!**/Migrations/**",
      "!**/Models/StudyEntry.cs"
    ]
  }
}
